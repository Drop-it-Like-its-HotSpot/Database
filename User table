CREATE TABLE "Users"
(
  "User_id" integer NOT NULL,
  "Email_id" character(80),
  "Latitude" double precision,
  "Longitude" double precision,
  "DisplayName" character(20),
  radius double precision,
  CONSTRAINT pk_user_id PRIMARY KEY ("User_id")
)
WITH (
  OIDS=FALSE
);
ALTER TABLE "Users"
  OWNER TO gators;


